{
    "compilerOptions": {
        "outDir": "./dist/",
        "strictNullChecks": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "noEmitHelpers": true,
        "importHelpers": true,
        "jsx": "react",
        "lib": [
            "dom",
            "es2015",
            "es2015.promise", 
            "es5"
        ]
    },
    "include": [
        "./src/**/*"
    ],
    "exclude":[
        "node_modules"
    ]

}